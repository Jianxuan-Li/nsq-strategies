{
  "name": "nsq-strategies",
  "version": "1.0.0",
  "description": "A nsq client with typical trategies in node.js",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "make test",
    "coverage": "istanbul cover _mocha -- test/*.test.js && istanbul check-coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wiredcraft/nsq-strategies.git"
  },
  "keywords": [
    "nsq"
  ],
  "author": "Wiredcraft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wiredcraft/nsq-strategies/issues"
  },
  "homepage": "https://github.com/Wiredcraft/nsq-strategies#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "bluebird": "3.7.2",
    "debug": "4.1.1",
    "lib-rest": "0.1.5",
    "nsqjs": "^0.12.1",
    "promise-retry": "^1.1.1",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/axios": "^0.14.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.5.1",
    "@types/promise-retry": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "coveralls": "3.0.9",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^3.1.0",
    "istanbul": "^0.4.5",
    "jest": "^28.1.0",
    "mocha": "6.2.2",
    "once": "^1.4.0",
    "prettier": "^2.3.2",
    "randexp": "0.5.3",
    "request": "2.88.0",
    "retry": "0.12.0",
    "should": "13.2.3",
    "sinon": "^12.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "make lint",
      "pre-push": "make lint"
    }
  }
}
