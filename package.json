{
  "name": "nsq-strategies",
  "version": "1.0.0",
  "description": "A nsq client with typical trategies in node.js",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --fix",
    "test": "make test",
    "coverage": "istanbul cover _mocha -- test/*.test.js && istanbul check-coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wiredcraft/nsq-strategies.git"
  },
  "keywords": [
    "nsq"
  ],
  "author": "Wiredcraft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wiredcraft/nsq-strategies/issues"
  },
  "homepage": "https://github.com/Wiredcraft/nsq-strategies#readme",
  "dependencies": {
    "bluebird": "3.7.2",
    "debug": "4.1.1",
    "file-register": "^0.1.0",
    "lib-rest": "0.1.5",
    "nsqjs": "^0.12.1",
    "promise-retry": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "chai": "4.2.0",
    "coveralls": "3.0.9",
    "eslint": "6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-wcl-backend": "^0.3.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "6.2.2",
    "once": "^1.4.0",
    "prettier": "^1.19.1",
    "randexp": "0.5.3",
    "request": "2.88.0",
    "retry": "0.12.0",
    "should": "13.2.3",
    "sinon": "^12.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "make lint",
      "pre-push": "make lint"
    }
  }
}
